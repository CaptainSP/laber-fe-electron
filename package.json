    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win --x64"
  },
  "author": "CaptainSP",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@google-cloud/speech": "^6.7.0",
