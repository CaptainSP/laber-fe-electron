    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.example.myapp",
    "mac": {
      "category": "public.app-category.utilities"
